    <div class="content-wrapper">

        <!-- 内容头部 -->
        <section class="content-header">
            <h1>
                教师课程管理 <small>学员成绩管理</small>
            </h1>
            <ol class="breadcrumb">
                <li><a href="#"><i class="fa fa-dashboard"></i> 首页</a></li>
                <li><a href="#">教师课程管理</a></li>
                <li class="active">学员成绩管理</li>
            </ol>
        </section>
        <!-- 内容头部 /-->

        <!-- 正文区域 -->
        <section class="content">

            <!-- .box-body -->
            <div class="box box-primary">
                <div class="box-header with-border">
                    <h3 class="box-title">批量添加成绩</h3>
                </div>

                <div class="box-body">

                    <!-- 数据表格 -->
                    <div class="table-box">

                        <!-- 工具栏 -->
                        <div class="pull-left" style="font-size: 24px; color: #ff5555">
                            ${checkResult}
                        </div>

                        <!-- 数据列表 -->
                        <form action="${pageContext.request.contextPath}/teacher/inputAllStuScore.do" method="post">
                            <table id="dataList" class="table table-bordered table-striped table-hover dataTable">
                                <thead>
                                <tr>
                                    <th>学号</th>
                                    <th>姓名</th>
                                    <th>性别</th>
                                    <th>学院</th>
                                    <th>班级</th>
                                    <th>成绩输入</th>
                                </tr>
                                </thead>
                                <tbody>
                                <c:forEach items="${studentList}" var="student">
                                    <tr>
                                        <td>${student.studentId}</td>
                                        <td>${student.name}</td>
                                        <td>${student.sex}</td>
                                        <td>${student.departmentId}</td>
                                        <td>${student.classId}</td>
                                        <td>
                                            <input type="text" class="form-control" name="score" value="${student.score}">
                                        </td>
                                    </tr>
                                </c:forEach>
                                </tbody>
                                <!--
                                    <tfoot>
                                    <tr>
                                    <th>Rendering engine</th>
                                    <th>Browser</th>
                                    <th>Platform(s)</th>
                                    <th>Engine version</th>
                                    <th>CSS grade</th>
                                    </tr>
                                    </tfoot>
                                -->
                            </table>
                            <!--数据列表/-->
                            <div class="pull-right">
                                <div class="form-group form-inline">
                                    <div class="btn-group">
                                        <button type="submit" class="btn btn-pinterest" title="刷新">
                                            确认输入
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </form>

                    </div>
                    <!-- 数据表格 /-->
                </div>
                <!-- /.box-body -->
            </div>
        </section>
